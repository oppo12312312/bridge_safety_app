<view class="user-manage"><view class="top"><uni-collapse vue-id="1" accordion="{{true}}" bind:__l="__l" vue-slots="{{['default']}}"><uni-collapse-item vue-id="{{('2')+','+('1')}}" title="员工信息" bind:__l="__l" vue-slots="{{['default']}}"><view class="list uni-list"><block wx:for="{{users}}" wx:for-item="item" wx:for-index="__i0__" wx:key="key"><label class="uni-list-cell uni-list-cell-pd"><view class="title">{{item.name}}</view><view class="text">{{userInfo[item.key]}}</view></label></block></view><button class="edit-password" type="default" data-event-opts="{{[['tap',[['editPassword',['$event']]]]]}}" bindtap="__e">修改密码</button></uni-collapse-item><uni-collapse-item vue-id="{{('3')+','+('1')}}" title="公司信息" bind:__l="__l" vue-slots="{{['default']}}"><view class="list uni-list"><block wx:for="{{deps}}" wx:for-item="item" wx:for-index="__i1__" wx:key="key"><label class="uni-list-cell uni-list-cell-pd"><view class="title">{{item.name}}</view><view class="text">{{depInfo[item.key]}}</view></label></block></view></uni-collapse-item></uni-collapse></view><button class="button" type="warn" data-event-opts="{{[['tap',[['exit',['$event']]]]]}}" bindtap="__e">退出登录</button></view>